citHeader("To cite RTDE in publications use:")


year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "gumbel: package for Gumbel copula",
         author = c(person("Christophe", "Dutang")),
         year = year,
         note = note,
         url = "https://CRAN.R-project.org/package=gumbel")



citEntry(entry="Manual",
         title = "gumbel: package for Gumbel copula",
         author = "Anne-Lise Caillat, Christophe Dutang, V\'eronique Larrieu and Triet NGuyen",
	 year = 2008,
         note = "R package version 1.01",

         textVersion =
	 paste(	"C. Dutang",
		"(2008),",
	       	"gumbel: package for Gumbel copula.")
)

